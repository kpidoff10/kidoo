{
  "i18n-ally.localesPaths": ["kidoo-app/i18n/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.defaultNamespace": "translation",
  "i18n-ally.namespace": false,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.sourceLanguage": "fr",
  "i18n-ally.displayLanguage": "fr",
  "i18n-ally.enabledFrameworks": ["react", "general"],
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.fullReloadOnChanged": true,
  "i18n-ally.usage.scanningIgnore": [
    "node_modules/**",
    "android/**",
    "ios/**",
    "esp32/**",
    "src/**"
  ],
}